                <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?><code>"its_org"</code>, a<?oxy_custom_end?>
            <olink targetdoc="../../attributes/itsm/itsm_org.xml" targetptr="itsm_org"
                    ><code>its_org</code></olink> string, <glossterm>optional</glossterm></member>
        <member>-
                <?oxy_custom_start type="oxy_content_highlight" color="140,255,140"?><code>"its_orgRef"</code>, a<?oxy_custom_end?>
